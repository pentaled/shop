<%- include('header'); -%>
    <div class="row">
        <% if(cart.length > 0){ %>
            <% cart.forEach((item) => { %>
                <div class="col-12">
                    <p><%= item.product_name %></p>
                </div>
            <% }); %>
            <% } else { %>
                <p>Your cart is empty.</p>
                <% }; %>
    </div>
<%- include('footer'); -%>